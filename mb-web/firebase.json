{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "**/!(*.js|*.css|*.json|*.svg|*.png|*.jpg|*.jpeg)",
        "function": "serve"
      }
    ]
  }
}
